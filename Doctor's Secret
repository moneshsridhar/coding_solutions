import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner s = new Scanner(System.in);
        int Length = s.nextInt();
        int Pages = s.nextInt();

        if(Length <= 23)
        {
            if(Pages>=500 && Pages<=1000)
                System.out.println("Take Medicine");
            else
                System.out.println("Don't take Medicine");
        }
        else
            System.out.println("Don't take Medicine");
    }
}
