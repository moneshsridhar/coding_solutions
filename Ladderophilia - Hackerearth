import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner s = new Scanner(System.in);
        int N = s.nextInt();

        for(int i=1; i<=N; i++)
        {
            System.out.println("*   *");
            System.out.println("*   *");
            System.out.println("*****");
        }
        System.out.println("*   *");
        System.out.println("*   *");
    }
}
